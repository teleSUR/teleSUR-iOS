+@implementation SHKMail
+
+#pragma mark -
+#pragma mark Configuration : Service Defination
+
++ (NSString *)sharerTitle
+{
+	return @"Email";
+}
+
++ (BOOL)canShareText
+{
+	return YES;
+}
+
++ (BOOL)canShareURL
+{
