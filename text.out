+{
+    [super dealloc];
+}
+
+-(void)TSMultimediaData:(TSMultimediaData *)data entidadesRecibidas:(NSArray *)array paraEntidad:(NSString *)entidad
+{
+    [self.noticias addObjectsFromArray:array];
+    
+    [self iniciarAnimacion];
+}
+
+@end
