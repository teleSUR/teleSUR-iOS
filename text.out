@@ -0,0 +1,340 @@
+//
+//  CDataScanner.m
+//  TouchCode
+//
+//  Created by Jonathan Wight on 04/16/08.
+//  Copyright 2008 toxicsoftware.com. All rights reserved.
+//
+//  Permission is hereby granted, free of charge, to any person
