+
+@implementation TSClipPlayerViewController
+
+@synthesize clip;
+
+- (id)initConClip:(NSDictionary *)diccionarioClip
+{
+    self = [super initWithContentURL:[NSURL URLWithString:[diccionarioClip valueForKey:@"archivo_url"]]];
+    if (self) {
+        self.clip = diccionarioClip;
+    }
+    return self;
+}
